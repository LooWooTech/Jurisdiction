@{
    ViewBag.Title = "审批历史";
    Layout = "~/Views/Manager/_Layout.cshtml";
    List<LoowooTech.Jurisdiction.Models.DataBook> List = ViewBag.List;
    int Serial = 1;
}

<table class="table table-bordered">
    <tr>
        <th>序号</th>
        <th>申请人</th>
        <th>申请组</th>
        <th>申请状态</th>
    </tr>
    @foreach (var item in List)
    {
        <tr>
            <td>@(Serial++)</td>
            <td>@(item.Name)</td>
            <td>@(item.GroupName)</td>
            <td></td>
        </tr>
    }
</table>
